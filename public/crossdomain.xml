<?xml version="1.0"?>
<!-- http://www.adobe.com/crossdomain.xml -->
<cross-domain-policy>
  <site-control permitted-cross-domain-policies="all"/>
  <allow-access-from domain="*.emotionrays.com" />
  <allow-access-from domain="*.oddimation.com" secure="false" />
  <allow-access-from domain="*.kidsplaymath.org" secure="false" />
  <allow-http-request-headers-from domain="*.oddimation.com" headers="*" secure="false"/>
  <allow-http-request-headers-from domain="*.kidsplaymath.org" headers="*" secure="false"/>
  <allow-http-request-headers-from domain="http://kidsplaymath.org" headers="*" secure="false"/>
</cross-domain-policy>
